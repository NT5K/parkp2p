{
  "name": "parkp2p",
  "version": "1.0.0",
  "description": "turn your driveway into a parking lot",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "concurrently \"npm run server\" \"npm run client\"",
